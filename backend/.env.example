# Copy this file to .env and fill in your values

# Server
PORT=5000
NODE_ENV=development

# MongoDB Atlas connection string
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.xxxxx.mongodb.net/oneshop?retryWrites=true&w=majority

